<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EmailDomain2__c</fullName>
    <description>Pulls The Company Domain From Email Address</description>
    <externalId>false</externalId>
    <formula>RIGHT( Email ,(LEN( Email )-FIND(&quot;@&quot;, Email )))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Pulls The Company Domain From Email Address</inlineHelpText>
    <label>Email Domain</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
