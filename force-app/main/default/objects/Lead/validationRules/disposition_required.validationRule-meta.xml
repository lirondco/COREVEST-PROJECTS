<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>disposition_required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL(Disposition__c, &quot;&quot;),
ispickval(Status, &apos;Unqualified&apos;)
)</errorConditionFormula>
    <errorDisplayField>Disposition__c</errorDisplayField>
    <errorMessage>Please fill out the Disposition field.</errorMessage>
</ValidationRule>
