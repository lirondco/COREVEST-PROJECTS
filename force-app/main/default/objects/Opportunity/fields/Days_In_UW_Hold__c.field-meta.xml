<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_In_UW_Hold__c</fullName>
    <description>Previously named &quot;Days in UW Hold.&quot;</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL(Date_Stage_Changed_To_UW_Hold__c), 0, TODAY() - Date_Stage_Changed_To_UW_Hold__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Number of days the deal has been in the most recent hold stage (either UW Hold or Processing Hold)</inlineHelpText>
    <label>Days In Hold Stage</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
